{
    "name": "make-it-app/laravel-favicon-extractor",
    "description": "A favicon extractor for Laravel",
    "keywords": [ "laravel", "favicon"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Kabin",
            "email": "ioffline@make-it.app"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.10",
        "graham-campbell/guzzle-factory": "^3.0|^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MakeIT\\LaravelFaviconExtractor\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x.x-dev"
        },
        "laravel": {
            "providers": [
                "MakeIT\\LaravelFaviconExtractor\\FaviconExtractorServiceProvider"
            ],
            "aliases": {
                "FaviconExtractor": "MakeIT\\LaravelFaviconExtractor\\Facades\\FaviconExtractor"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "support": {
        "issues": "https://github.com/make-it-app/laravel-favicon-extractor/issues",
        "source": "https://github.com/make-it-app/laravel-favicon-extractor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
